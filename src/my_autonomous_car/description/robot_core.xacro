<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="inertial_macros.xacro"/>
  <xacro:property name="pi" value="3.141592653589793"/>

  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <material name="orange">
    <color rgba="1 0.3 0.1 1"/>
  </material>

  <material name="blue">
    <color rgba="0.2 0.2 1 1"/>
  </material>

  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>

  <!-- BASE LINK -->
  <link name="base_link">
    <origin xyz="0 0 0.07 "/>
  </link>

  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_footprint"/>
    <origin xyz="0 0 0" rqt="0 0 0"/>
  </joint>

  <link name="base_footprint"/>

  <!-- BACK CHASSIS LINK -->
  <joint name="back_chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="back_chassis"/>
    <origin xyz="-0.05 0 -0.02 "/>
  </joint>

  <link name="back_chassis">
    <visual>
      <origin xyz="0.08 0 0.025"/>
      <geometry>
        <box size="0.16 0.12 0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin xyz="0.08 0 0.025"/>
      <geometry>
        <box size="0.16 0.12 0.05"/>
      </geometry>
    </collision>

    <xacro:inertial_box mass="0.9" x="0.16" y="0.12" z="0.05">
      <origin xyz="0.08 0 0.025" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>

  <gazebo reference="back_chassis">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- FRONT CHASSIS LINK -->
  <joint name="chassis_front_joint" type="fixed">
    <parent link="back_chassis"/>
    <child link="front_chassis"/>
    <origin xyz="0.16 0 0 "/>
  </joint>

  <link name="front_chassis">
    <visual>
      <origin xyz="0.06 0 0.025"/>
      <geometry>
        <box size="0.12 0.085 0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin xyz="0.06 0 0.025"/>
      <geometry>
        <box size="0.12 0.085 0.05"/>
      </geometry>
    </collision>

    <xacro:inertial_box mass="0.6" x="0.12" y="0.085" z="0.05">
      <origin xyz="0.06 0 0.025" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>

  <gazebo reference="front_chassis">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- STEERING ROD -->
  <joint name="steering_rod_joint" type="fixed">
    <parent link="base_link"/>
    <child link="steering_rod"/>
    <origin xyz="0.17 0 0.005"/>
  </joint>

  <link name="steering_rod">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.015 0.12 0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.015 0.12 0.05"/>
      </geometry>
    </collision>

    <xacro:inertial_box mass="0.25" x="0.015" y="0.12" z="0.05">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_box>
  </link>

  <gazebo reference="steering_rod">
    <material>Gazebo/Black</material>
  </gazebo>

  <!-- LEFT BACK WHEEL'S AXIS -->
  <joint name="left_back_axis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="left_back_axis"/>
    <origin xyz="0 0.06 0" rpy="-${pi/2} 0 0"/>
  </joint>

  <link name="left_back_axis">
    <visual>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.02" length="0.01" radius="0.005">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_back_axis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- LEFT BACK WHEEL -->
  <joint name="left_back_wheel_joint" type="continuous">
    <parent link="left_back_axis"/>
    <child link="left_back_wheel"/>
    <origin xyz="0 0 0.0175"/>
    <axis xyz="0 0 1"/>
    <limit effort="5.0" velocity="50.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="left_back_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.12" length="0.025" radius="0.0325">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_back_wheel">
    <material>Gazebo/White</material>
    <mu>1.2</mu>
    <mu2>1.0</mu2>
  </gazebo>

  <!-- RIGHT BACK AXIS -->
  <joint name="right_back_axis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="right_back_axis"/>
    <origin xyz="0 -0.06 0" rpy="${pi/2} 0 0"/>
  </joint>

  <link name="right_back_axis">
    <visual>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.02" length="0.01" radius="0.005">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_back_axis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- RIGHT BACK WHEEL -->
  <joint name="right_back_wheel_joint" type="continuous">
    <parent link="right_back_axis"/>
    <child link="right_back_wheel"/>
    <origin xyz="0 0 0.0175"/>
    <axis xyz="0 0 -1"/>
    <limit effort="5.0" velocity="50.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="right_back_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.12" length="0.025" radius="0.0325">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_back_wheel">
    <material>Gazebo/White</material>
    <mu>1.2</mu>
    <mu2>1.0</mu2>
  </gazebo>

  <!-- FRONT LEFT AXIS -->
  <joint name="left_front_axis_joint" type="revolute">
    <parent link="base_link"/>
    <child link="left_front_axis"/>
    <origin xyz="0.17 0.06 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.541052" upper="0.698132" effort="5.0" velocity="2.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="left_front_axis">
    <visual>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.02" length="0.01" radius="0.005">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_front_axis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- FRONT LEFT WHEEL -->
  <joint name="left_front_wheel_joint" type="continuous">
    <parent link="left_front_axis"/>
    <child link="left_front_wheel"/>
    <origin xyz="0 0.0175 0" rpy="-${pi/2} 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="5.0" velocity="50.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="left_front_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.12" length="0.025" radius="0.0325">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="left_front_wheel">
    <material>Gazebo/White</material>
    <mu>1.2</mu>
    <mu2>1.0</mu2>
  </gazebo>

  <!-- FRONT RIGHT AXIS -->
  <joint name="right_front_axis_joint" type="revolute">
    <parent link="base_link"/>
    <child link="right_front_axis"/>
    <origin xyz="0.17 -0.06 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.698132" upper="0.541052" effort="5.0" velocity="2.0"/>
    <dynamics damping="0.5" friction="0.1"/>
  </joint>

  <link name="right_front_axis">
    <visual>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
      <material name="orange"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.005" length="0.01"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.02" length="0.01" radius="0.005">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_front_axis">
    <material>Gazebo/Orange</material>
  </gazebo>

  <!-- FRONT RIGHT WHEEL -->
  <joint name="right_front_wheel_joint" type="continuous">
    <parent link="right_front_axis"/>
    <child link="right_front_wheel"/>
    <origin xyz="0 -0.0175 0" rpy="${pi/2} 0 0"/>
    <axis xyz="0 0 -1"/>
    <limit effort="5.0" velocity="50.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="right_front_wheel">
    <visual>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
      <material name="white"/>
    </visual>

    <collision>
      <geometry>
        <cylinder radius="0.0325" length="0.025"/>
      </geometry>
    </collision>

    <xacro:inertial_cylinder mass="0.12" length="0.025" radius="0.0325">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
  </link>

  <gazebo reference="right_front_wheel">
    <material>Gazebo/White</material>
    <mu>1.2</mu>
    <mu2>1.0</mu2>
  </gazebo>

</robot>
